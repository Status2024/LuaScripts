			HumanoidState.Speed = 89
			buttonSpeed.Text = "Speed: "..HumanoidState.Speed
			buttonSpeed.BackgroundColor3 = GuiColor.On_Color_R
			HumanoidState.TmpSpeed = HumanoidState.Speed
			coroutine.wrap(WRP.applySpeed)()
		end
		
